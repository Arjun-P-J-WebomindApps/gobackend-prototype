scalar UUID
scalar Time

type User{
    id: UUID
    name:String!
    username:String!
    email:String!
    password:String!
    mobile:String!
    role:Int!
    isActive:Boolean!
    createdAt:Time!
    updatedAt:Time!
    deletedAt:Time
}

type Query{
    users:[User!]!
}

input CreateUserInput{
    name:String!
    username:String!
    email:String!
    password:String!
    mobile:String!
    role:Int!
    isActive:Boolean!
}

type Mutation{
    createUser(input:CreateUserInput!):User!
}